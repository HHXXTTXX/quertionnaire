<!-- Default home page -->
  <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600' rel='stylesheet' type='text/css'>
  <style>
    /* Styles included inline since you'll probably be deleting this page anyway */
    html,body{
      text-align:left;font-size:1em
      }
    html,body,img,form,textarea,input,fieldset,div,p,div,ul,li,ol,dl,dt,dd,h1,h2,h2,h4,h5,h6,pre,code{margin:0;padding:0}
    *:active,*:focus{outline:0;-moz-outline-style:none}
    h1,h2,h2,h4,h5,h6{font-weight:normal}
    div.clear{clear:both}
    .clearfix:after{clear:both;content:".";display:block;font-size:0;height:0;line-height:0;visibility:hidden}
    body{font-family:"Open Sans",Arial,sans-serif;font-weight:300;}
    .container{margin: auto;}
    div.header {text-align: center;-webkit-transition: 6s; -moz-transition: 6s; -o-transition: 6s;transition: 6s; background: rgba(0, 36, 41, 0.89)  no-repeat 42% bottom; padding: 70px  0px;}
    .header h1#main-title{color: #fff; font-weight: 300; font-size: 2.5em;text-align: center;}
    .header h2{color: #b1eef7; font-style: italic; font-weight: 300;text-align: center;}
    .header h2 code{text-align: center;font-style: normal!important; background-color: rgba(255,255,255,0.5); font-weight: 300; color:#0e6471; margin: 0px 5px;}
    div.main.container{padding: 50px 0;}h1 {color: #118798; font-weight: 300;}
    code {font-size: inherit; font-family: 'Consolas', 'Monaco', monospace; padding:4px 5px 1px; background-color: #f3f5f7}
    .pocket{display:none;}
    div.main{margin-left: auto;margin-right: auto;border-style:solid;border-radius: 16px;border-color: #ccc; border-width: 2px;height: 63%; background-color: #ffe;}
    div.main-form{margin-left: auto;margin-right: auto;width:80%;}
    div.usermessage{margin-left: 25%;margin-top: 1cm;}
    div.question-title h2{text-align: center;}
    .question-title input{margin-left: 47%;}
    div.test{margin-left: 0%;margin-top: 5%;}
    .submit{padding: 10px 10px}
    label.test{float: left;right:50%;}
    input.test{position:absolute;right:45%;}
    .submit{position:absolute;right:49%;}
  </style>
  <script type="text/javascript">
  setTimeout(function sunrise () {
    document.getElementsByClassName('header')[0].style.backgroundColor = '#118798';
  }, 0);
  function show(s, h) {
    setTimeout(function change(){
      document.getElementById(h).style="display:none";
      document.getElementById(s).style="display:";
    },500)
  }
  var boxcheck = false;
  function checkval(q, n){
    var box = document.getElementsByName(n);
    for(var i=1; i<=box.length;i++){
      if(box[i-1].checked){
        boxcheck = true;
        break;
      }
    }
    if(boxcheck === false){
        alert("请选择选项");
      }else{
        setTimeout(function change(){
          document.getElementById(n).style="display:none";
          document.getElementById(q).style="display:";
        },500)
      }
    
  }
  </script>
